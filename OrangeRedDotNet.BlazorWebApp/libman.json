{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.1.3",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "font-awesome@6.1.1",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css"
      ]
    },
    {
      "library": "Chart.js@3.7.1",
      "destination": "wwwroot/lib/Chart.js/",
      "files": [
        "chart.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@videojs/http-streaming@2.14.2",
      "destination": "wwwroot/lib/videojs/http-streaming/",
      "files": [
        "dist/videojs-http-streaming.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "video.js@7.20.2",
      "destination": "wwwroot/lib/video.js/",
      "files": [
        "dist/video.min.js",
        "dist/video-js.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "videojs-contrib-quality-levels@2.1.0",
      "destination": "wwwroot/lib/videojs-contrib-quality-levels/",
      "files": [
        "dist/videojs-contrib-quality-levels.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "videojs-hls-quality-selector@1.1.4",
      "destination": "wwwroot/lib/videojs-hls-quality-selector/",
      "files": [
        "dist/videojs-hls-quality-selector.min.js"
      ]
    }
  ]
}